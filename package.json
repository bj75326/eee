{
  "name": "eeeditor",
  "version": "0.0.1",
  "decription": "A rich text editor based on draft.js & antd",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "father:compile": "npx father build",
    "father:start": "npx father doc dev --storybook",
    "father:build": "npx father doc build  --storybook",
    "gh-pages": "cnpm run build && gh-pages -d dist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@draft-js-plugins/editor": "^4.0.0-beta4",
    "antd": "^4.9.4",
    "classnames": "^2.2.6",
    "draft-js": "^0.11.7"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/draft-js": "^0.10.44",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.3",
    "father": "^2.13.4",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^4.1.2",
    "umi": "^3.3.3",
    "vfile-message": "^2.0.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "resolutions": {
    "@types/react": "^16.14.2"
  },
  "homepage": "https://bj75326.github.com/eeeditor"
}
